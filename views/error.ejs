<% layout('layouts/boilerplate')%>
<div class="row justify-content-center">
    <div class="col-6">
        <div class="alert alert-danger" role="alert">
            <h1 class="alert-heading"><%= err.message %></h1>
            <% if (process.env.NODE_ENV !== "production") { %> 
            <p><%= err.stack %></p>
            <% } %>
        </div>
    </div>
</div>